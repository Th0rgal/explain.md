{
  "name": "explain-md-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit --incremental false",
    "test": "vitest run",
    "benchmark:cache": "tsx scripts/proof-cache-benchmark.ts --out ../../docs/benchmarks/proof-cache-benchmark.json",
    "benchmark:tree-a11y": "tsx scripts/tree-a11y-evaluation.ts --out ../../docs/benchmarks/tree-a11y-evaluation.json",
    "benchmark:tree-a11y:ci": "tsx scripts/tree-a11y-evaluation.ts --baseline ../../docs/benchmarks/tree-a11y-evaluation.json --out ../../.explain-md/tree-a11y-evaluation-report.json",
    "benchmark:verification-replay": "tsx scripts/verification-replay-evaluation.ts --out ../../docs/benchmarks/verification-replay-evaluation.json",
    "benchmark:verification-replay:ci": "tsx scripts/verification-replay-evaluation.ts --baseline ../../docs/benchmarks/verification-replay-evaluation.json --out ../../.explain-md/verification-replay-evaluation-report.json",
    "benchmark:observability-slo": "tsx scripts/observability-slo-benchmark.ts --out ../../docs/benchmarks/observability-slo-benchmark.json",
    "benchmark:observability-slo:ci": "tsx scripts/observability-slo-benchmark.ts --baseline ../../docs/benchmarks/observability-slo-benchmark.json --out ../../.explain-md/observability-slo-benchmark-report.json"
  },
  "dependencies": {
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.17.14",
    "@types/three": "^0.183.1",
    "next": "^14.2.32",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.183.1"
  },
  "devDependencies": {
    "@types/node": "^24.4.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.32",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
